"<placeholder-1/> /* * call-seq: * expression[name] -&gt; 変数の値 * "
"expression[offset] -&gt; 変数の値 * * _expression_で使用可能な変数のうち、名"
"前が_name_または * _offset_番目にExpression#append_objectされた変数の値を返 "
"* す。 */ static VALUE rb_grn_expression_array_reference (VALUE self, VALUE "
"rb_name_or_offset) { grn_ctx *context = NULL; grn_obj *expression, "
"*variable, *value; char *name = NULL; unsigned name_size = 0; int offset; "
"rb_grn_expression_deconstruct(SELF(self), &amp;expression, &amp;context, "
"NULL, NULL, NULL, NULL, NULL); switch (TYPE(rb_name_or_offset)) { case "
"T_STRING: name = RSTRING_PTR(rb_name_or_offset); name_size = RSTRING_LEN"
"(rb_name_or_offset); variable = grn_expr_get_var(context, expression, name, "
"name_size); return GRNBULK2RVAL(context, variable, NULL, self); break; case "
"T_FIXNUM: offset = NUM2INT(rb_name_or_offset); value = "
"grn_expr_get_var_by_offset(context, expression, offset); return GRNBULK2RVAL"
"(context, value, NULL, self); break; default: rb_raise(rb_eArgError, \"xxx"
"\"); break; } return Qnil; }"
msgstr ""

