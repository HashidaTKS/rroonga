"<placeholder-1/> static VALUE rb_grn_expression_append_object (int argc, "
"VALUE *argv, VALUE self) { VALUE rb_object, rb_operation, rb_n_arguments; "
"grn_ctx *context = NULL; grn_obj *expression, *object; grn_operator "
"operation = GRN_OP_PUSH; int n_arguments = 1; rb_scan_args(argc, argv, "
"\"12\", &amp;rb_object, &amp;rb_operation, &amp;rb_n_arguments); if (!NIL_P"
"(rb_operation)) operation = NUM2INT(rb_operation); if (!NIL_P"
"(rb_n_arguments)) n_arguments = NUM2INT(rb_n_arguments); "
"rb_grn_expression_deconstruct(SELF(self), &amp;expression, &amp;context, "
"NULL, NULL, NULL, NULL, NULL); object = RVAL2GRNOBJECT(rb_object, &amp;"
"context); grn_expr_append_obj(context, expression, object, operation, "
"n_arguments); rb_grn_context_check(context, self); rb_ary_push(rb_iv_get"
"(self, \"@objects\"), rb_object); return self; }"
