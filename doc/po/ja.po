"self); if (!NIL_P(exception)) { rb_grn_object_close(rb_cursor); rb_iv_set"
"(self, \"cursor\", Qnil); rb_exc_raise(exception); } if (object) { rb_yield"
"(GRNOBJECT2RVAL(Qnil, context, object, GRN_FALSE)); } } rb_grn_object_close"
"(rb_cursor); rb_iv_set(self, \"cursor\", Qnil); return Qnil; }"
