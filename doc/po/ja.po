"groonga側が * 未実装のため、現在は無視される） */ static VALUE "
"rb_grn_table_lock (int argc, VALUE *argv, VALUE self) { grn_id id = "
"GRN_ID_NIL; grn_ctx *context; grn_obj *table; int timeout = 0; grn_rc rc; "
"VALUE options, rb_timeout, rb_id; rb_scan_args(argc, argv, \"01\", &amp;"
"options); rb_grn_table_deconstruct(SELF(self), &amp;table, &amp;context, "
"NULL, NULL, NULL, NULL, NULL, NULL); rb_grn_scan_options(options, \"timeout"
"\", &amp;rb_timeout, \"id\", &amp;rb_id, NULL); if (!NIL_P(rb_timeout)) "
"timeout = NUM2UINT(rb_timeout); if (!NIL_P(rb_id)) id = NUM2UINT(rb_id); rc "
"= grn_obj_lock(context, table, id, timeout); rb_grn_context_check(context, "
"self); rb_grn_rc_check(rc, self); if (rb_block_given_p()) { return rb_ensure"
"(rb_yield, Qnil, rb_grn_table_unlock_ensure, self); }"
