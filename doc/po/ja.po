"&lt; n_keys; i++) { VALUE rb_sort_options, rb_key; if (RVAL2CBOOL"
"(rb_obj_is_kind_of(rb_group_keys[i], rb_cHash))) { rb_sort_options = "
"rb_group_keys[i]; } else { rb_sort_options = rb_hash_new(); rb_hash_aset"
"(rb_sort_options, RB_GRN_INTERN(\"key\"), rb_group_keys[i]); } "
"rb_grn_scan_options(rb_sort_options, \"key\", &amp;rb_key, NULL); if "
"(RVAL2CBOOL(rb_obj_is_kind_of(rb_key, rb_cString))) { VALUE resolved_rb_key; "
"resolved_rb_key = rb_grn_table_get_column(self, rb_key); if (NIL_P"
