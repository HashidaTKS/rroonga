"(context, &amp;id, RSTRING_PTR(rb_id), RSTRING_LEN(rb_id)); accessor = "
"grn_obj_column(context, view, RSTRING_PTR(rb_name), RSTRING_LEN(rb_name)); "
"grn_obj_get_value_o(context, accessor, &amp;id, value); grn_obj_unlink"
"(context, accessor); rb_value = GRNOBJ2RVAL(Qnil, context, value, self); "
