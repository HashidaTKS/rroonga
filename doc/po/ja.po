"column, &amp;context, NULL, NULL, NULL, NULL, &amp;range_id, NULL, NULL, "
"NULL); n = RARRAY_LEN(rb_sources); rb_source_values = RARRAY_PTR"
"(rb_sources); sources = ALLOCA_N(grn_id, n); for (i = 0; i &lt; n; i++) "
"{ sources[i] = resolve_source_id(context, column, range_id, rb_source_values"
"[i]); }"
