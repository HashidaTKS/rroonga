"<placeholder-1/> static VALUE rb_grn_table_group (int argc, VALUE *argv, "
"VALUE self) { grn_ctx *context = NULL; grn_obj *table; grn_table_sort_key "
"*keys; grn_table_group_result *results; int i, n_keys, n_results; grn_rc rc; "
