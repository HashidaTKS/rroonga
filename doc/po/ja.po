"VALUE rb_grn_table_clear_lock (int argc, VALUE *argv, VALUE self) { grn_id "
"id = GRN_ID_NIL; grn_ctx *context; grn_obj *table; VALUE options, rb_id; "
"rb_scan_args(argc, argv, \"01\", &amp;options); rb_grn_table_deconstruct(SELF"
"(self), &amp;table, &amp;context, NULL, NULL, NULL, NULL, NULL, NULL); "
"rb_grn_scan_options(options, \"id\", &amp;rb_id, NULL); if (!NIL_P(rb_id)) "
"id = NUM2UINT(rb_id); grn_obj_clear_lock(context, table); return Qnil; }"
