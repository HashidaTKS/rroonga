"rb_grn_table_key_support_get_id (int argc, VALUE *argv, VALUE self) { VALUE "
"rb_key; rb_scan_args(argc, argv, \"01\", &amp;rb_key); if (NIL_P(rb_key)) "
"{ return rb_grn_object_get_id(self); } else { grn_id id; id = "
"rb_grn_table_key_support_get(self, rb_key); if (id == GRN_ID_NIL) { return "
