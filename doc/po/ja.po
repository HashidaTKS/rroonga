"(context, column, range_id, rb_source_values[i]); } { grn_obj bulk_sources; "
"GRN_OBJ_INIT(&amp;bulk_sources, GRN_BULK, 0, GRN_ID_NIL); GRN_TEXT_SET"
"(context, &amp;bulk_sources, sources, n * sizeof(grn_id)); rc = "
"grn_obj_set_info(context, column, GRN_INFO_SOURCE, &amp;bulk_sources); "
"exception = rb_grn_context_to_exception(context, self); grn_obj_unlink"
"(context, &amp;bulk_sources); } if (!NIL_P(exception)) rb_exc_raise"
"(exception); rb_grn_rc_check(rc, self); return Qnil; }"
